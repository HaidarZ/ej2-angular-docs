@(Html.EJS().Schedule("schedule")
    .Width("100%")
    .Height("550px")
    .Group(group => group.Resources(ViewBag.Resources))
    .Resources(res => { res.DataSource(ViewBag.Owners)
        .Field("OwnerId")
        .Title("Owners")
        .Name("Owners")
        .TextField("text")
        .IdField("id")
        .ColorField("color")
        .Add();
    })
    .Render()
)