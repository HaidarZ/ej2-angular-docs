
            <ejs-stockchart id="stockchart" load="stockload" title="AAPL Stock Price">
                <e-stockchart-series-collection>
                    <e-stockchart-series type='Candle' xName="x" yName="open"> </e-stockchart-series>
                </e-stockchart-series-collection>
            </ejs-stockchart>

    <script src="~/financial-data.js"></script>
    <script>
        var data = chartData;
            function stockload(args) {
                args.stockChart.series[0].dataSource = data;
                args.stockChart.primaryXAxis.valueType ='DateTime' ;
                args.stockChart.primaryYAxis={
                    valueType: 'Double'
                };

            }
    </script>